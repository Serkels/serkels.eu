{
  "name": "@1/strapi-trpc-router",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "private": true,
  "scripts": {
    "dev": "pnpm run build --watch --preserveWatchOutput ",
    "build:copy": "yes | cp -rf ../../@1/strapi-trpc-router/index.ts ./lib/index.ts",
    "build:tsc": "tsc ./lib/index.ts --outDir ./lib --declaration --declarationMap",
    "build": "pnpm run build:copy && pnpm run  build:tsc"
  },
  "dependencies": {
    "@trpc/server": "10.37.1",
    "superjson": "1.13.1",
    "ws": "8.13.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "prettier": "3.0.2",
    "typescript": "5.1.6"
  }
}
