{
  "private": true,
  "scripts": {
    "xXx:cp:config": "cp -rf ../@1/config/typescript-config ./xxx/config/",
    "xXx:cp:core": "cp -rf ../@1/core/lib ./xxx/core/",
    "xXx:cp:models": "cp -rf ../@1/models/index.ts ./xxx/models/",
    "xXx:cp:modules:exchange": "cp -rf ../@1/modules/exchange ./xxx/modules/",
    "xXx:cp:modules:profile": "cp -rf ../@1/modules/profile ./xxx/modules/",
    "xXx:cp:strapi-openapi": "cp -rf ../@1/strapi-openapi/v1.d.ts ./xxx/strapi-openapi/v1.d.ts",
    "xXx:cp:strapi-trpc-router": "cp -rf ../@1/strapi-trpc-router/index.ts ./xxx/strapi-trpc-router/index.ts",
    "watch:cp:config": "pnpx chokidar-cli \"../@1/config/**/*.json\" -c \"pnpm run '/xXx:cp:config/'\" --initial",
    "watch:cp:core": "pnpx chokidar-cli \"../@1/core/lib/**/*.ts\" -c \"pnpm run '/xXx:cp:core/'\" --initial",
    "watch:cp:models": "pnpx chokidar-cli \"../@1/models/index.ts\" -c \"pnpm run '/xXx:cp:models/'\" --initial",
    "watch:cp:modules": "pnpx chokidar-cli \"../@1/modules/**/*.ts\" -i \"../@1/**/node_modules/*\" -c \"pnpm run '/xXx:cp:modules:.*/'\" --initial",
    "watch:cp:strapi-openapi": "pnpx chokidar-cli \"../@1/strapi-openapi/v1.d.ts\" -c \"pnpm run '/xXx:cp:strapi-openapi/'\" --initial",
    "watch:cp:strapi-trpc-router": "pnpx chokidar-cli \"../@1/strapi-trpc-router/index.ts\" -c \"pnpm run '/xXx:cp:strapi-trpc-router/'\" --initial",
    "dev:workspace": "pnpm run -r --parallel dev ",
    "dev:xXx": "pnpm run /watch:cp:.*/",
    "dev": "pnpm run /dev:.*/",
    "build": "pnpm run -r build",
    "start": "pnpm --filter strapi start"
  },
  "engines": {
    "node": ">=18"
  }
}
