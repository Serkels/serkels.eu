{
  "kind": "collectionType",
  "collectionName": "questions",
  "info": {
    "singularName": "question",
    "pluralName": "questions",
    "displayName": "Question",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "is_accepted": {
      "type": "boolean",
      "default": false,
      "required": false
    },
    "accepted_answer": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::comments.comment"
    },
    "title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "profile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-profile.user-profile"
    },
    "answer_count": {
      "type": "integer",
      "min": 0
    },
    "edited_at": {
      "type": "datetime"
    },
    "last_activity": {
      "type": "datetime"
    }
  }
}
