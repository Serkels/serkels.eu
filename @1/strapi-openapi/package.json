{
  "name": "@1/strapi-openapi",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build:ts": "rm -rf v1.d.ts && openapi-typescript openapi.json --output v1.d.ts --empty-objects-unknown --default-non-nullable --alphabetize",
    "dev": "pnpx chokidar-cli \"*.json\" -c \"pnpm run build:ts\" --initial --follow-symlinks"
  },
  "devDependencies": {
    "openapi-typescript": "6.5.2",
    "prettier": "3.0.2",
    "typescript": "5.1.6"
  }
}
