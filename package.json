{
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "cd admin/strapi && npm start",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --parallel --continue",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "prettier": "3.0.3",
    "turbo": "1.10.13",
    "typescript": "5.2.2"
  },
  "pnpm": {
    "overrides": {
      "supports-color": "^8.1.1",
      "zod": "3.22.2"
    }
  },
  "workspaces": [
    "@1/config/*",
    "@1/core",
    "@1/modules",
    "@1/strapi-openapi",
    "@1/strapi-trpc-router",
    "@1/ui",
    "packages/@douglasduteil/datatypes...hours-to-seconds",
    "www"
  ]
}
