{
  "private": true,
  "engines": {
    "node": "20.6.1",
    "npm": "9.8.1",
    "pnpm": "8.7.5"
  },
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --parallel --continue",
    "heroku-prebuild": "pnpx turbo prune --scope=strapi && cd out",
    "heroku-postbuild": "turbo run build --filter=strapi",
    "lint": "turbo run lint",
    "start": "pnpm -r start"
  },
  "devDependencies": {
    "prettier": "3.0.3",
    "turbo": "1.10.13",
    "typescript": "5.2.2"
  },
  "pnpm": {
    "overrides": {
      "supports-color": "^8.1.1",
      "zod": "3.22.2"
    }
  }
}
