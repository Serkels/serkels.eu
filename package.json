{
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --parallel --continue",
    "heroku-prebuild": "pnpx turbo prune --scope=strapi && rm -rf 01 www admin && pnpx cpx \"out/**/*\" .",
    "heroku-postbuild": "turbo run build --filter=strapi",
    "lint": "turbo run lint",
    "start": "pnpm -r start"
  },
  "devDependencies": {
    "prettier": "3.0.3",
    "turbo": "1.10.14",
    "typescript": "5.2.2"
  },
  "optionalDependencies": {
    "formik": "2.4.5",
    "mime": "2.6.0",
    "supports-color": "8.1.1",
    "ws": "8.14.1",
    "websockets": "npm:ws@8.14.1",
    "zod": "3.22.2"
  },
  "pnpm": {
    "overrides": {
      "formik": "$formik",
      "mime": "$mime",
      "supports-color": "$supports-color",
      "ws": "$ws",
      "websockets": "$websockets",
      "zod": "$zod"
    }
  }
}
