{
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=9",
    "pnpm": ">=8"
  },
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev --parallel --continue",
    "heroku-prebuild": "pnpx turbo prune --scope=api && ls -la && echo && ls -la out && rm -rf apps packages out/patches && pnpx cpx \"out/**/*\" .",
    "heroku-postbuild": "turbo run build --filter=api",
    "lint": "turbo run lint",
    "start": "pnpm -r start"
  },
  "devDependencies": {
    "prettier": "3.0.3",
    "turbo": "1.10.14",
    "typescript": "5.2.2"
  },
  "pnpm": {
    "patchedDependencies": {
      "react-nest@0.1.10": "patches/react-nest@0.1.10.patch"
    }
  }
}
