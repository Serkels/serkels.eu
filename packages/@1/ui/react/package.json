{
  "name": "@1.ui/react",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./*": {
      "types": "./src/*/index.tsx",
      "default": "./src/*/index.tsx"
    },
    "./*/atom": {
      "types": "./src/*/atom.ts",
      "default": "./src/*/atom.ts"
    },
    "./*/types": {
      "types": "./src/*/types.ts",
      "default": "./src/*/types.ts"
    },
    "./tailwind.config.ts": {
      "types": "./tailwind.config.ts",
      "default": "./tailwind.config.ts"
    },
    "./icons": {
      "types": "./src/icons/index.ts",
      "default": "./src/icons/index.ts"
    },
    "./input/*": {
      "types": "./src/input/*.tsx",
      "default": "./src/input/*.tsx"
    },
    "./form/atom": {
      "types": "./src/form/atom.ts",
      "default": "./src/form/atom.ts"
    },
    "./form": {
      "types": "./src/form/index.ts",
      "default": "./src/form/index.ts"
    },
    "./form/*": {
      "types": "./src/form/*.tsx",
      "default": "./src/form/*.tsx"
    },
    "./motion/types": {
      "types": "./src/motion/types.ts",
      "default": "./src/motion/types.ts"
    },
    "./motion/*": {
      "types": "./src/motion/*.tsx",
      "default": "./src/motion/*.tsx"
    },
    "./useragent": {
      "types": "./src/useragent/index.ts",
      "default": "./src/useragent/index.ts"
    },
    "./react-query": {
      "types": "./src/react-query/index.ts",
      "default": "./src/react-query/index.ts"
    }
  },
  "scripts": {
    "type-check": "tsc --build"
  },
  "prettier": "@1.config/prettier/tailwindcss.js",
  "dependencies": {
    "@1.modules/core": "workspace:*",
    "@react-hook/merged-ref": "1.3.2",
    "@react-hookz/web": "24.0.4",
    "bowser": "2.11.0",
    "date-fns": "4.1.0",
    "framer-motion": "11.11.11",
    "react": "18.3.1",
    "react-aria-components": "1.4.1",
    "react-slotify": "0.2.2",
    "tailwind-variants": "0.3.0",
    "ts-pattern": "5.5.0"
  },
  "devDependencies": {
    "@1.config/tailwindcss": "workspace:*",
    "@1.config/typescript": "workspace:*",
    "@tanstack/react-query": "4.36.1",
    "@testing-library/react": "16.0.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3"
  }
}
