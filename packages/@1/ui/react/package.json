{
  "name": "@1.ui/react",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./tailwind.config.ts": {
      "types": "./tailwind.config.ts",
      "default": "./tailwind.config.ts"
    },
    "./*": {
      "types": "./src/*/index.tsx",
      "default": "./src/*/index.tsx"
    },
    "./*/atom": {
      "types": "./src/*/atom.ts",
      "default": "./src/*/atom.ts"
    },
    "./icons": {
      "types": "./src/icons/index.ts",
      "default": "./src/icons/index.ts"
    },
    "./input/*": {
      "types": "./src/input/*.tsx",
      "default": "./src/input/*.tsx"
    }
  },
  "scripts": {
    "type-check": "tsc --build"
  },
  "dependencies": {
    "@1.modules/core": "workspace:*",
    "@react-hook/merged-ref": "1.3.2",
    "@react-hookz/web": "23.1.0",
    "date-fns": "2.30.0",
    "formik": "2.4.5",
    "framer-motion": "10.16.4",
    "react-aria-components": "1.0.0-beta.2",
    "tailwind-variants": "0.1.14"
  },
  "devDependencies": {
    "@1.config/prettier": "workspace:*",
    "@1.config/tailwindcss": "workspace:*",
    "@1.config/typescript": "workspace:*",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "prettier": "3.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  }
}
