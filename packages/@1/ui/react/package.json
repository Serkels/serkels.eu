{
  "name": "@1.ui/react",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./tailwind.config.ts": {
      "types": "./tailwind.config.ts",
      "default": "./tailwind.config.ts"
    },
    "./*": {
      "types": "./src/*/index.tsx",
      "default": "./src/*/index.tsx"
    },
    "./*/atom": {
      "types": "./src/*/atom.ts",
      "default": "./src/*/atom.ts"
    },
    "./icons": {
      "types": "./src/icons/index.ts",
      "default": "./src/icons/index.ts"
    },
    "./input/*": {
      "types": "./src/input/*.tsx",
      "default": "./src/input/*.tsx"
    },
    "./form/atom": {
      "types": "./src/form/atom.ts",
      "default": "./src/form/atom.ts"
    },
    "./form": {
      "types": "./src/form/index.ts",
      "default": "./src/form/index.ts"
    },
    "./form/*": {
      "types": "./src/form/*.tsx",
      "default": "./src/form/*.tsx"
    },
    "./motion/*": {
      "types": "./src/motion/*.tsx",
      "default": "./src/motion/*.tsx"
    }
  },
  "scripts": {
    "type-check": "tsc --build"
  },
  "dependencies": {
    "@1.modules/core": "workspace:*",
    "@react-hook/merged-ref": "1.3.2",
    "@react-hookz/web": "24.0.4",
    "date-fns": "3.6.0",
    "framer-motion": "11.3.17",
    "react-aria-components": "1.3.1",
    "react-nest": "0.1.10",
    "react-slotify": "0.2.2",
    "tailwind-variants": "0.2.1",
    "ts-pattern": "5.2.0"
  },
  "devDependencies": {
    "@1.config/prettier": "workspace:*",
    "@1.config/tailwindcss": "workspace:*",
    "@1.config/typescript": "workspace:*",
    "@tanstack/react-query": "4.36.1",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.7",
    "typescript": "5.5.4"
  }
}
