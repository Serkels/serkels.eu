{
  "name": "@1.ui/react",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./tailwind.config.ts": {
      "types": "./tailwind.config.ts",
      "default": "./tailwind.config.ts"
    },
    "./*": {
      "types": "./src/*/index.tsx",
      "default": "./src/*/index.tsx"
    },
    "./*/atom": {
      "types": "./src/*/atom.ts",
      "default": "./src/*/atom.ts"
    },
    "./icons": {
      "types": "./src/icons/index.ts",
      "default": "./src/icons/index.ts"
    },
    "./input/*": {
      "types": "./src/input/*.tsx",
      "default": "./src/input/*.tsx"
    },
    "./form/atom": {
      "types": "./src/form/atom.ts",
      "default": "./src/form/atom.ts"
    },
    "./form/*": {
      "types": "./src/form/*.tsx",
      "default": "./src/form/*.tsx"
    }
  },
  "scripts": {
    "type-check": "tsc --build"
  },
  "dependencies": {
    "@1.modules/core": "workspace:*",
    "@react-hook/merged-ref": "1.3.2",
    "@react-hookz/web": "24.0.4",
    "react-slotify": "0.2.2",
    "date-fns": "3.3.1",
    "formik": "2.4.5",
    "framer-motion": "11.0.5",
    "ts-pattern": "5.0.8",
    "react-aria-components": "1.1.1",
    "tailwind-variants": "0.2.0"
  },
  "devDependencies": {
    "@1.config/prettier": "workspace:*",
    "@1.config/tailwindcss": "workspace:*",
    "@1.config/typescript": "workspace:*",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "@tanstack/react-query": "4.36.1",
    "prettier": "3.2.5",
    "tailwindcss": "3.3.5",
    "typescript": "5.3.3"
  }
}
