{
  "name": "@1.infra/database",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./seed": {
      "types": "./seed/index.ts",
      "default": "./seed/index.ts"
    },
    "./testing": {
      "types": "./testing/index.ts",
      "default": "./testing/index.ts"
    },
    ".": {
      "types": "./index.ts",
      "default": "./index.ts"
    }
  },
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "dev:studio": "prisma studio",
    "format": "prisma format",
    "generate": "prisma generate",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "push": "prisma db push --skip-generate",
    "seed": "prisma db seed",
    "studio": "prisma studio",
    "type-check": "tsc --build"
  },
  "prettier": "@1.config/prettier",
  "dependencies": {
    "@1.modules/notification.domain": "workspace:*",
    "@prisma/client": "5.19.1"
  },
  "devDependencies": {
    "@1.config/typescript": "workspace:*",
    "@electric-sql/pglite": "0.2.7",
    "@faker-js/faker": "8.4.1",
    "@types/node": "22.5.0",
    "date-fns": "3.6.0",
    "dedent": "1.5.3",
    "pglite-prisma-adapter": "0.2.0",
    "prisma": "5.19.1",
    "slugify": "1.6.6",
    "tsx": "4.18.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
