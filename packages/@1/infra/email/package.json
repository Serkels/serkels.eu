{
  "name": "@1.infra/email",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "scripts": {
    "dev": "tsc --build --watch --preserveWatchOutput",
    "type-check": "tsc --build"
  },
  "prettier": "@1.config/prettier",
  "dependencies": {
    "@react-email/render": "1.0.1",
    "debug": "4.3.7",
    "nodemailer": "6.9.15",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@1.config/typescript": "workspace:*",
    "@types/debug": "4.1.12",
    "@types/nodemailer": "6.4.16",
    "@types/react": "18.3.11"
  }
}
