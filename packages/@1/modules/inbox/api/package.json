{
  "name": "@1.modules/inbox.api",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#internal/*": {
      "import": "./dist/internal/*.mjs",
      "require": "./dist/internal/*.cjs"
    }
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "type-check": "tsc --build"
  },
  "prettier": "@1.config/prettier",
  "dependencies": {
    "@1.modules/core": "workspace:*",
    "@1.modules/inbox.domain": "workspace:*",
    "@1.modules/notification.domain": "workspace:*",
    "@1.modules/trpc": "workspace:*",
    "date-fns": "4.1.0",
    "ts-pattern": "5.5.0",
    "typed-emitter": "2.1.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@1.config/typescript": "workspace:*",
    "@1.infra/database": "workspace:*",
    "typescript": "5.7.2"
  }
}
