{
  "name": "@1.modules/core",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "dev": "tsc --build --watch --preserveWatchOutput",
    "clean": "rm -rf .turbo tsconfig.tsbuildinfo",
    "type-check": "tsc --build"
  },
  "prettier": "@1.config/prettier",
  "dependencies": {
    "@sentry/core": "8.54.0",
    "date-fns": "4.1.0",
    "debug": "4.4.0",
    "error-custom-class": "10.0.0",
    "zod": "3.24.1",
    "zohar": "1.0.1"
  },
  "devDependencies": {
    "@1.config/typescript": "workspace:*",
    "typescript": "5.7.3"
  }
}
