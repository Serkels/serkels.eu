{
  "name": "@1.modules/core",
  "version": "0.1.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "dev": "tsc --build --watch --preserveWatchOutput",
    "type-check": "tsc --build"
  },
  "dependencies": {
    "debug": "4.3.4",
    "error-custom-class": "10.0.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@1.config/typescript": "workspace:*",
    "typescript": "5.3.3"
  }
}
