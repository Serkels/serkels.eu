    "type-check": "tsc --build"
  },
  "prettier": "@1.config/prettier",
  "dependencies": {
    "@sentry/core": "8.33.1",
    "date-fns": "4.1.0",
    "debug": "4.3.7",
    "error-custom-class": "10.0.0",
    "zod": "3.23.8",
    "zohar": "1.0.1"
  },
  "devDependencies": {
    "@1.config/typescript": "workspace:*",
    "typescript": "5.6.2"
  }
}
