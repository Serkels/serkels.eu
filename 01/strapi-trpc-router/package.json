{
  "name": "@1/strapi-trpc-router",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "module": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./src/index.ts"
    },
    "./*": {
      "module": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "types": "./src/*.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "types": "./src/index.ts",
  "scripts": {
    "build:cjs": "npx swc src -C module.type=commonjs -d dist/cjs",
    "build:esm": "npx swc src -C module.type=es6 -d dist/esm",
    "build": "pnpm run /build:.*/",
    "dev": "pnpm run build --watch"
  },
  "dependencies": {
    "@1/core": "workspace:*",
    "@1/modules": "workspace:*",
    "@1/strapi-openapi": "workspace:*",
    "@trpc/server": "10.38.3",
    "superjson": "1.13.1",
    "ws": "8.14.2",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@1/typescript-config": "workspace:*",
    "@types/node": "20.6.3",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  }
}
