{
  "name": "@1/core_",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "module": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./src/index.ts"
    },
    "./*": {
      "module": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js",
      "import": "./dist/esm/*.js",
      "types": "./src/*.ts"
    }
  },
  "main": "./dist/cjs/index.js",
  "types": "./src/index.ts",
  "scripts": {
    "build:cjs": "npx swc src -C module.type=commonjs -d dist/cjs",
    "build:esm": "npx swc src -C module.type=es6 -d dist/esm",
    "build": "pnpm run /build:.*/",
    "dev": "pnpm run build --watch"
  },
  "dependencies": {
    "@1/next-tsyringe": "workspace:*",
    "@trpc/server": "10.38.5",
    "debug": "4.3.4",
    "error-custom-class": "9.0.0",
    "react": "18.2.0",
    "reflect-metadata": "0.1.13",
    "rich-domain": "1.18.4",
    "superjson": "1.13.1",
    "tsyringe": "4.8.0",
    "uuid": "9.0.1",
    "zod": "3.22.2"
  },
  "devDependencies": {
    "@1/typescript-config": "workspace:*",
    "openapi-fetch": "0.7.7",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  }
}
